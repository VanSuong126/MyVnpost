    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "i18next": "^22.0.3",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.21",
    "react": "17.0.2",
